using System;

namespace Лабораторная__3
{
    class Program
    {
        static void Main(string[] args)
        {
            double w, b, t, x, y, Se;

            Console.Write("Введите w >");
            w = Convert.ToSingle(Console.ReadLine());

            Console.Write("Введите b >");
            b = Convert.ToSingle(Console.ReadLine());

            Console.Write("Введите t >");
            t = Convert.ToSingle(Console.ReadLine());

            Console.Write("Введите x >");
            x = Convert.ToSingle(Console.ReadLine());

            Console.Write("Введите y >");
            y = Convert.ToSingle(Console.ReadLine());

            Console.Write("Введите w >");
            w = Convert.ToSingle(Console.ReadLine());

            Se = Math.Sqrt(w * 111) + Math.Sin(b * t) - x + y * w;
            Console.WriteLine("\n\n");

            Console.WriteLine("Денежный формат {0:С} \n", Se);
            Console.WriteLine("Экспоменциальный формат {0:E} \n", Se);
            Console.WriteLine("Формат с фиксированной запятой {0:F3, 3} \n", Se);
            Console.WriteLine("Общий формат {0:G} \n", Se);
            Console.WriteLine("Числовой формат {0:N} \n", Se);
            Console.WriteLine("Процентный формат {0:P} \n", Se);
        }
    }
}
